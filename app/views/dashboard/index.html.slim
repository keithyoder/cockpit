- content_for :head
    script[src="https://ajax.googleapis.com/ajax/libs/jquery/3.7.1/jquery.min.js"]
    link[rel="preconnect" href="https://fonts.googleapis.com"]
    link[rel="preconnect" href="https://fonts.gstatic.com" crossorigin]
    link[href="https://fonts.googleapis.com/css2?family=Orbitron:wght@400..900&display=swap" rel="stylesheet"]
    link[href="https://fonts.googleapis.com/css2?family=Rubik+Mono+One&display=swap" rel="stylesheet"]
    link[rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.6.0/css/all.min.css" crossorigin="anonymous" referrerpolicy="no-referrer"]
    link[rel="stylesheet" href="https://cdn.jsdelivr.net/gh/lipis/flag-icons@7.2.3/css/flag-icons.min.css"]

body
  .container
    .part1
      .time-div
        span
          i.fa-solid.fa-clock
        span.time

      .date-div
        span
          i.fa-solid.fa-calendar-days
        span.date

      .temperature-div
        span
          i.fa-solid.fa-temperature-low
        span#outdoor_temperature
          | 21Â° C

      .humidity-div
        span
          i.fa-solid.fa-droplet
        span#humidity
          | 80%

      .location-div
        span
          i.fa-solid.fa-location-dot
        span
          i.fi.fi-ar
        span
          | New York
      .phone-icons-div
        .satellite-div
          span
            i.fa-solid.fa-satellite
        .gsm-signal-div
          span
            i.fa-solid.fa-signal
        .battery-phone-div
          span
            i.fa-solid.fa-battery-full
          span
            | 100%
    .part2
      .signal-left
        i.fa-solid.fa-circle-arrow-left
      .direction
        .div1
          span.span1
            i.fa-solid.fa-diamond-turn-right
          br
          span.span2
            | 204 m
        .div2
          span.span1
            | Turn left
          hr
          span.span2
            | Castleton Ave
      .signal-right
        i.fa-solid.fa-circle-arrow-right
    .part3
      .tachometer
        .tachometer-numbers
          .tachometer-item.first
            | 0
